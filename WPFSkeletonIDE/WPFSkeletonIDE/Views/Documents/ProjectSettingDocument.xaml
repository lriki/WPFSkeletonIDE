<UserControl x:Class="WPFSkeletonIDE.Views.Documents.ProjectSettingDocument"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:WPFSkeletonIDE.Views.Documents"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
             Padding="5">
    <Grid>
        <StackPanel>
            <TextBlock Margin="5">ProjectSettingDocument</TextBlock>
            <TextBox Margin="5">Text</TextBox>
            <CheckBox Content="Check" Margin="5"/>
            <Button Content="Button"  Margin="5"/>
        </StackPanel>
    </Grid>
</UserControl>
